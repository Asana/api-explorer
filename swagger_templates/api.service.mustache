{{#operations}}
/// <reference path="../interfaces.ts" />

/* tslint:disable:max-line-length */
/* tslint:disable:eofline */
var resourceBase = <Resource>{
  "name": "{{toSnakeCase pathPrefix}}",
  "comment": "",
  "properties":[],
  "actions": [
  {{#operation}}{{^formParams}}
  {{#contents}}
  {
    "name": "{{operationId}}",
    "method": "{{toUpperCase httpMethod}}",
    "path": "{{parsePath path}}",
    "params": [
      {{#pathParams}} 
      {
      "name": "{{baseName}}",
      "type": "{{dataType}}",
      "example_values": [{{&getExampleFromJson jsonSchema}}],
      "comment": "{{description}}",
      "required": {{required}}
      },
      {{/pathParams}}
      {{#queryParams}}
      {
      "name": "{{baseName}}",
      "type": "{{dataType}}",
      "example_values": [{{&getExampleFromJson jsonSchema}}],
      "comment": "{{description}}",
      "required": {{required}}
      },
      {{/queryParams}}
    ],
    "comment": "{{summary}}"
  },
  {{/contents}}
  {{/formParams}}{{/operation}}
  ]
}
{{/operations}}
export = resourceBase;